      <plugin name='diff_drive' filename='libgazebo_ros_diff_drive.so'>

        <ros>
          <!-- <namespace>/r2d2</namespace> -->
          <!-- <remapping>cmd_vel:=cmd_vel</remapping> -->
          <!-- <remapping>odom:=odom</remapping> -->
        </ros>

        <update_rate>30</update_rate>
        <command_topic>cmd_vel</command_topic>

        <!-- wheels -->
        <left_joint>left_leg_base_to_left_center_wheel</left_joint>
        <right_joint>right_leg_base_to_right_center_wheel</right_joint>

        <!-- kinematics -->
        <wheel_separation>0.4</wheel_separation>
        <wheel_diameter>0.04</wheel_diameter>

        <!-- limits -->
        <max_wheel_torque>20</max_wheel_torque>
        <max_wheel_acceleration>1.0</max_wheel_acceleration>

        <!-- output -->
        <publish_odom>true</publish_odom>
        <publish_odom_tf>true</publish_odom_tf>
        <publish_wheel_tf>false</publish_wheel_tf>

        <odometry_topic>odom</odometry_topic>
        <odometry_frame>odom</odometry_frame>
        <robot_base_frame>base_footprint</robot_base_frame>

      </plugin>



  <!-- laser -->
  <joint name="base_to_laser" type="fixed">
    <parent>base_link</parent>
    <child>base_scan</child>
  </joint>
  <link name="base_scan">
    <pose relative_to="base_link">0.11 0 -0.3 0 0 0</pose>
    <sensor name="laser" type="ray">
      <ray>
        <scan>
          <horizontal>
            <samples>180</samples>
            <resolution>1.0</resolution>
            <min_angle>-1.57</min_angle>
            <max_angle>1.57</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.2</min>
          <max>10.0</max>
          <resolution>0.015000</resolution>
        </range>
      </ray>
      <always_on>true</always_on>
      <update_rate>100</update_rate>
      <visualize>true</visualize>             
      <plugin name="laserscan" filename="libgazebo_ros_ray_sensor.so">
        <ros>
          <remapping>~/out:=scan</remapping>
        </ros>
        <output_type>sensor_msgs/LaserScan</output_type>
        <frame_name>base_scan</frame_name>
      </plugin>
    </sensor>

  </link>


